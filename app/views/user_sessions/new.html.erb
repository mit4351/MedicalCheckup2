<h1>ログイン</h1>

<%= form_for(:user_session, url: user_session_path) do |f| %>

<table border="1">
<tr>
<th><%= f.label :enpno, '被保険者番号' %></th>
<td><%= f.text_field :enpno %></td>
</tr>
<tr>
<th><%= f.label :password, 'パスワード' %></th>
<td><%= f.password_field :password %></td>
</tr>
</table>

<%= f.submit 'ログインする' %>

<% end %>